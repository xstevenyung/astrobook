---
import Layout from "../layouts/Layout.astro";
import Frame from "../components/Frame.astro";
import { getStories } from "../utils/components";

const stories = await getStories();
---

<Layout title="Welcome to Astro.">
  <main class="mx-auto max-w-5xl flex flex-col gap-y-10 py-20">
    {
      stories.map((story) => {
        return <Frame {...story} />;
      })
    }
  </main>
</Layout>
